<h1>SUPPPPPPPP</h1>
<% @messages.each do |message| %>
  <div class="message">
    <h3><a href="/messages/<%= message.id %>"><small>(by: <%= message.author %>)</small></h3>
    <p>
      <%= message.content %>
    </p>
    <p>
      <%= message.url %>
    </p>

    <p>
      Created at: <%= message.created_at %>
    </p>
  </div>
<% end %>

<a href="/messages/new">Post a message... It's FREE (for now)!</a>